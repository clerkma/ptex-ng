rm -f chktest

(bld=`pwd`
  cd $srcdir/$CHKTEX_TREE || exit 1
  CHKTEX_CONFIG=chktexrc \
    $bld/chktex -mall -r -g0 -lchktexrc -v5 Test.tex || exit 1
) >chktest || exit 1

diff $srcdir/$CHKTEX_TREE/Test.posix-ere.out chktest || exit 1

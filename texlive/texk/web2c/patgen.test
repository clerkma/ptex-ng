BinDir=${BinDir:-.}
ExeExt=${ExeExt:-}
_patgen=$BinDir/patgen$ExeExt

test -d tests || mkdir -p tests

$_patgen $srcdir/tests/dict $srcdir/tests/patterns tests/xpatout \
  $srcdir/tests/translate <$srcdir/tests/patgen.in || exit 1


	printf '\%s{Latin scripts}' "chapter" >>$@
	echo '\ienc{cp1252}' >>$@
	echo '\section{Codepage 1252}' >>$@
	for i in $(DOCS_CP1252); do echo "\\input{$$i}" >>$@; done

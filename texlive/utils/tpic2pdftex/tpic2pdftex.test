for f in beamerexample example; do
	pic -z -t $srcdir/$f.pic >$f.tpic || exit 77
	$srcdir/tpic2pdftex $f.tpic >$f.tex || exit 1
	# There may be legitimate diffs due to floating point rounding.
	diff $srcdir/$f.exp $f.tex || :
done

